<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.willdarden.group2project.AddResources">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_marginEnd="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="16dp"
        android:text="Add Resources to Event"
        android:textColor="@android:color/black"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/AddResourcesToEvent"
        android:layout_width="99dp"
        android:layout_height="32dp"
        android:layout_marginEnd="142dp"
        android:layout_marginStart="143dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginRight="142dp"
        android:layout_marginLeft="143dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="83dp"
        android:layout_height="24dp"
        android:layout_marginEnd="261dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:text="Meal Type"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginRight="261dp"
        android:layout_marginLeft="40dp" />

    <EditText
        android:id="@+id/addResources_mealtype_plaintext"
        android:layout_width="215dp"
        android:layout_height="46dp"
        android:layout_marginEnd="129dp"
        android:layout_marginStart="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="breakfast,lunch,or dinner"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        android:layout_marginRight="129dp"
        android:layout_marginLeft="40dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_marginEnd="244dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:text="Food Venue "
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addResources_mealtype_plaintext"
        android:layout_marginRight="244dp"
        android:layout_marginLeft="40dp" />

    <EditText
        android:id="@+id/addResources_foodvenue_plaintext"
        android:layout_width="215dp"
        android:layout_height="46dp"
        android:layout_marginEnd="129dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="food venue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginRight="129dp"
        android:layout_marginLeft="40dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="119dp"
        android:layout_height="25dp"
        android:layout_marginEnd="225dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:text="Meal Formality"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addResources_foodvenue_plaintext"
        android:layout_marginRight="225dp"
        android:layout_marginLeft="40dp" />

    <EditText
        android:id="@+id/addResources_mealformality_plaintext"
        android:layout_width="215dp"
        android:layout_height="46dp"
        android:layout_marginEnd="130dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="formal or informal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_marginRight="130dp"
        android:layout_marginLeft="40dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginEnd="286dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:text="Drink Venue"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addResources_mealformality_plaintext"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="286dp" />

    <EditText
        android:id="@+id/addResources_dringkvenue_plaintext"
        android:layout_width="215dp"
        android:layout_height="46dp"
        android:layout_marginEnd="129dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="standard or alcohol"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        android:layout_marginRight="129dp"
        android:layout_marginLeft="40dp" />

    <Button
        android:id="@+id/addResources_addpage_button"
        android:layout_width="122dp"
        android:layout_height="36dp"
        android:layout_marginEnd="131dp"
        android:layout_marginStart="131dp"
        android:text="Add"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addResources_dringkvenue_plaintext" />

    <Button
        android:id="@+id/button4"
        android:layout_width="122dp"
        android:layout_height="36dp"
        android:layout_marginEnd="148dp"
        android:layout_marginStart="148dp"
        android:text="logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
