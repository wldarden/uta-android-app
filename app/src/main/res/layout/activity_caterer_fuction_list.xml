<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.willdarden.group2project.CatererFuctionList">

    <TextView
        android:id="@+id/FunctionList_textview"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_marginEnd="87dp"
        android:layout_marginStart="87dp"
        android:layout_marginTop="57dp"
        android:text="Function List"
        android:textColor="@android:color/black"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/CatererViewEventCalendar_button"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginEnd="104dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="113dp"
        android:text="View event calendar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FunctionList_textview" />

    <Button
        android:id="@+id/logout_button"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginBottom="45dp"
        android:layout_marginEnd="102dp"
        android:layout_marginStart="102dp"
        android:text="logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />
</android.support.constraint.ConstraintLayout>
